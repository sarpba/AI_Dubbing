{
  "enviroment": "sync",
  "script": "ASR/elevenlabs/elevenlabs.py",
  "api": "evenlabs",
  "description": "Evenlabs ASR API használata a projekthez tartozó hangfájlok feldolgozására, normalizált szó szintű (word_segments) JSON kimenettel.",
  "required": [
    {
      "name": "project_name",
      "flags": [
        "-p",
        "--project-name"
      ],
      "type": "option",
      "default": null
    }
  ],
  "optional": [
    {
      "name": "language",
      "flags": [
        "--language"
      ],
      "type": "option",
      "default": null
    },
    {
      "name": "model",
      "flags": [
        "--model"
      ],
      "type": "option",
      "default": "scribe_v1_experimental"
    },
    {
      "name": "api_url",
      "flags": [
        "--api-url"
      ],
      "type": "option",
      "default": "https://api.elevenlabs.io/v1/speech-to-text"
    },
    {
      "name": "api_key",
      "flags": [
        "--api-key"
      ],
      "type": "option",
      "default": null
    },
    {
      "name": "debug",
      "flags": [
        "--debug"
      ],
      "type": "flag",
      "default": false
    }
  ]
}
