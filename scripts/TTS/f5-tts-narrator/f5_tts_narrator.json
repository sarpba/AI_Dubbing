{
  "enviroment": "f5-tts",
  "script": "TTS/f5-tts-narrator/f5_tts_narrator.py",
  "description": "Narrátor referencia alapján generálja az összes szinkron szegmenst F5-TTS-sel.",
  "required": [
    {
      "name": "project_name",
      "type": "positional",
      "default": null
    },
    {
      "name": "norm",
      "flags": [
        "--norm"
      ],
      "type": "option",
      "default": null
    },
    {
      "name": "narrator",
      "flags": [
        "--narrator"
      ],
      "type": "option",
      "default": null
    }
  ],
  "optional": [
    {
      "name": "model_dir",
      "flags": [
        "--model_dir"
      ],
      "type": "option",
      "default": null
    },
    {
      "name": "speed",
      "flags": [
        "--speed"
      ],
      "type": "option",
      "default": 1.0
    },
    {
      "name": "nfe_step",
      "flags": [
        "--nfe_step"
      ],
      "type": "option",
      "default": 32
    },
    {
      "name": "remove_silence",
      "flags": [
        "--remove_silence"
      ],
      "type": "flag",
      "default": false
    },
    {
      "name": "phonetic_ref",
      "flags": [
        "--phonetic-ref"
      ],
      "type": "flag",
      "default": false
    },
    {
      "name": "normalize_ref_audio",
      "flags": [
        "--normalize-ref-audio"
      ],
      "type": "flag",
      "default": false
    },
    {
      "name": "eq_config",
      "flags": [
        "--eq-config"
      ],
      "type": "option",
      "default": null
    },
    {
      "name": "ref_audio_peak",
      "flags": [
        "--ref-audio-peak"
      ],
      "type": "option",
      "default": 0.95
    },
    {
      "name": "max_workers",
      "flags": [
        "--max_workers"
      ],
      "type": "option",
      "default": null
    },
    {
      "name": "seed",
      "flags": [
        "--seed"
      ],
      "type": "option",
      "default": -1
    },
    {
      "name": "max_retries",
      "flags": [
        "--max-retries"
      ],
      "type": "option",
      "default": 5
    },
    {
      "name": "tolerance_factor",
      "flags": [
        "--tolerance-factor"
      ],
      "type": "option",
      "default": 1.0
    },
    {
      "name": "min_tolerance",
      "flags": [
        "--min-tolerance"
      ],
      "type": "option",
      "default": 2
    },
    {
      "name": "whisper_model",
      "flags": [
        "--whisper-model"
      ],
      "type": "option",
      "default": "openai/whisper-large-v3"
    },
    {
      "name": "beam_size",
      "flags": [
        "--beam-size"
      ],
      "type": "option",
      "default": 5
    },
    {
      "name": "save_failures",
      "flags": [
        "--save-failures"
      ],
      "type": "flag",
      "default": false
    },
    {
      "name": "keep_best_over_tolerance",
      "flags": [
        "--keep-best-over-tolerance"
      ],
      "type": "flag",
      "default": false
    },
    {
      "name": "debug",
      "flags": [
        "--debug"
      ],
      "type": "flag",
      "default": false
    }
  ]
}
