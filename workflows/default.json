    {
      "script": "ASR/parakeet-tdt-0.6b-v2.py",
      "enabled": true,
      "halt_on_fail": true,
      "params": {}
    },
    {
      "script": "DIARIZE/split_segments_by_speaker_codex.py",
      "enabled": true,
      "halt_on_fail": true,
      "params": {}
